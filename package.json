    "bs-react-test-renderer": "^2.0.0",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-test-renderer": "16.8.6",
    "reason-react": "0.7.0",
    "reason-react-native": "0.60.0"
  },
  "jest": {
    "preset": "react-native",
    "snapshotResolver": "<rootDir>/snapshotResolver.js"
